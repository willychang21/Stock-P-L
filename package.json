    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "decimal.js": "^10.4.3",
    "express": "^5.2.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
